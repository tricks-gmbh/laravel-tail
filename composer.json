{
    "name": "spatie/laravel-tail",
    "description": "Easily tail application logs",
    "keywords": [
        "laravel",
        "tail",
        "log",
        "laravel-tail",
        "development"
    ],
    "homepage": "https://github.com/spatie/laravel-tail",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.3",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "illuminate/console": "~5.8.0|^6.0|^7.0",
        "symfony/process": "^4.0|^5.0",
        "spatie/ssh": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^4.6|^5.0",
        "phpunit/phpunit": "^9.0"

    },
    "autoload": {
        "psr-4": {
            "Spatie\\Tail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Tail\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Tail\\TailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
